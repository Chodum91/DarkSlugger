const oV = {
    props: {
        theme: {
            type: String,
            default: "grey",
            validator(e) {
                return ["grey", "greyBorder"].includes(e)
            }
        },
        toLink: {
            type: [String, Object],
            required: !0
        },
        icon: {
            type: String,
            default: ""
        },
        iconColor: {
            type: String,
            default: "text-blue-600"
        }
    },
    computed: {
        classes() {
            switch (this.theme) {
            case "grey":
            default:
                return ["!text-blue-900", "disabled:text-grey-500", "hover:bg-grey-300", "active:bg-grey-500"];
            case "greyBorder":
                return ["border", "border-grey-300", "!text-blue-900", "disabled:text-grey-500", "hover:bg-grey-300", "active:bg-grey-500"]
            }
        },
        tag() {
            return typeof this.toLink == "object" && this.$router ? "router-link" : "BaseLink"
        },
        linkProperties() {
            return this.tag === "router-link" ? {
                to: this.toLink
            } : {
                href: this.toLink
            }
        }
    }
