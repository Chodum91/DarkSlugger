            type: Array,
            required: !0,
            validator(e) {
                return e.every(t=>{
                    const n = t.userId ? "userId" : "id";
                    return $t(["type", n], t)
                }
                )
            }
        }
    },
    emits: ["select"],
    data() {
        return {
            members: [],
            sectionIsExpanded: !1
        }
    },
    async created() {
        this.updateMembers(this.item.type, this.item.id)
    },
    methods: {
        setExpandedStatus(e) {
            this.sectionIsExpanded = e
        },
        updatePermissions(e) {
            var t;
            if ((t = this.permissions[e.type === Kr ? n0 : t0]) != null && t[e.userId || e.id]) {
                const n = this.permissions[e.type === Kr ? n0 : t0][e.userId || e.id];
                return this.permissionsOptions.find(r=>r.numeric === parseInt(n)).verb
            }
            return ""
        },
        selectItem(e) {
            this.$emit("select", e)
        },
        isSelectedItem(e) {
            return this.selectedItems.findIndex(t=>(t.userId || t.id) === (e.userId || e.id) && t.type === e.type) !== -1
        },
        async updateMembers(e, t) {
            if (e !== Kr) {
                const {data: n} = await oF(t)
                  , r = n.map(a=>ve(V({}, a), {
                    type: Kr,
                    userId: a.id
                }));
                this.members = r.sort((a,s)=>(a.name.toLowerCase() < s.name.toLowerCase() ? -1 : 1) * 1)
            }
        }
    }
