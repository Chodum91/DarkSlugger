var PY = ({threadSearchBarRef: e="threadSearchBar"}={})=>({
    data: ()=>({
        threads: [],
        totalThreads: null,
        status: "ready",
        filters: {
            input: "",
            filter: []
        }
    }),
    computed: {
        unfetchedThreads() {
            return !this.totalThreads || !this.threads ? 0 : Q6(this.totalThreads, this.threads)
        }
    },
    mounted() {
        if (this.$refs[e]) {
            const t = this.$refs[e].searchItems;
            this.filters.filter = [t[0].items[0]],
            t[1] && this.filters.filter.push(t[1].items[0])
        }
    },
    watch: {
        filters: {
            deep: !0,
            handler() {
                this.fetchThreads(!1, this.threadId)
            }
        }
    },
    methods: {
        async fetchThreads(t=!1, n=null) {
            this.status = "loading";
            try {
                const r = n ? {
                    selectedThreadId: n
                } : {}
                  , {threads: a, totalThreads: s} = await OY({
                    threads: t ? this.threads : [],
                    filters: this.filters,
                    loadThreadsParams: r,
                    countThreadsParams: {
                        taskId: this.taskId
                    }
                });
                this.threads = a,
                this.totalThreads = s,
                this.status = "ready"
            } catch {
                this.status = "ready",
                this.$notify({
                    title: this.translate("labels.error"),
                    text: this.translate("error.threadLoading"),
                    type: "error"
                })
            }
        },
        loadMoreThreads(t=null) {
            this.fetchThreads(!0, t)
        }
    }
});
