const i8 = {
    components: {
        FlatButton: nt,
        DownloadPageMediaLink: yC,
        PageThumbnail: n8,
        ExpandedAudioPlayer: eC,
        RotatableImage: s8
    },
    props: {
        page: {
            type: Object,
            required: !0
        },
        extraParams: {
            type: Object,
            default: ()=>({})
        },
        angle: {
            type: Number,
            default: 0
        },
        showAudioMedia: {
            type: Boolean,
            default: !1
        },
        loading: {
            type: Boolean,
            default: !1
        },
        canDownload: {
            type: Boolean,
            default: !1
        }
    },
    emits: ["angle", "replace", "rotate"],
    computed: {
        isImage() {
            return !!(this.page.type.search("image") !== -1 || this.isAudioImage)
        },
        isAudioImage() {
            return this.slideType === "audio" && !this.showAudioMedia
        },
        slideType() {
            return cr(this.page)
        },
        audioUrl() {
            return this.slideType !== "audio" ? null : Xm(this.page)
        },
        canRotate() {
            return !this.isImage || this.loadingOrProcessing ? !1 : !this.page.media.some(e=>e.default)
        },
        mediaType() {
            return this.isImage ? "image" : this.slideType
        },
        replacePayload() {
            return this.slideType === "video" ? "video" : this.slideType === "image" ? GR : this.isAudioImage ? "image" : "audio"
        },
        loadingOrProcessing() {
            return this.loading || Gl(this.page.mediaStatus)
        }
    }
}
