var XW = Z(WW, [["render", YW]]);
const JW = 2
  , ZW = {
    components: {
        ExportButton: VW,
        SeparatorBar: Pg,
        DialogFooterBar: _i,
        Loading: Xr,
        ThreadLastExported: XW,
        A11yAnnouncer: vo
    },
    props: {
        thread: {
            type: Object,
            required: !0,
            validator: e=>$t(["id"], e)
        }
    },
    data: ()=>({
        exportBlocked: !0,
        exportJob: null,
        exportChecker: null,
        exportsForThread: [],
        credits: null
    }),
    computed: {
        status() {
            var e;
            return ((e = this.exportJob) == null ? void 0 : e.status) || kW
        },
        createdAgo() {
            var e;
            return (e = this.exportJob) != null && e.creation ? y3(this.exportJob.creation, this) : 0
        },
        lastCompletedExport() {
            return this.exportsForThread.length ? MW(this.exportsForThread).sort((e,t)=>t.creation - e.creation)[0] : null
        },
        canExport() {
            return !this.exportBlocked && this.hasCredits
        },
        hasCredits() {
            var e, t;
            return ((e = this.credits) == null ? void 0 : e.unlimited) || ((t = this.credits) == null ? void 0 : t.total) > 0
        }
    },
    async created() {
        await this.getUserCredits(),
        await this.checkForExports(),
        this.exportBlocked = !1
    },
    methods: {
        async attemptExport() {
            if (!this.hasCredits) {
                FW();
                return
            }
            if (!!this.canExport) {
                this.exportBlocked = !0;
                try {
                    const {data: e} = await PW(this.thread.id);
                    this.exportJob = e,
                    this.$nextTick(()=>{
                        this.exportJob && this.$el.focus()
                    }
                    ),
                    this.startCheckingExport()
                } catch (e) {
                    bt(e, this)
                } finally {
                    this.exportBlocked = !1
                }
            }
        },
        async checkForExports() {
            const {data: e} = await BW(this.thread.id);
            if (this.exportsForThread = e,
            !this.exportsForThread.length) {
                this.exportJob = null;
                return
            }
            const t = RW(this.exportsForThread);
            if (t.length) {
                this.exportJob = t[0],
                this.startCheckingExport();
                return
            }
            this.exportJob = null
        },
        startCheckingExport() {
            this.exportChecker || (this.exportChecker = setInterval(this.updateExport, JW * 1e3))
        },
        async updateExport() {
            try {
                const {data: e} = await DW(this.exportJob.id);
                if (this.exportJob = e,
                this.exportJob.status !== nS)
                    return;
                clearInterval(this.exportChecker),
                await this.checkForExports(),
                this.$refs.announcer.announce(this.translate("labels.exportFinished"))
            } catch (e) {
                clearInterval(this.exportChecker),
                bt(e, this)
            }
        },
        async getUserCredits() {
            var e;
            try {
                const {data: t} = await nF({
                    properties: ["resources"]
                })
                  , n = t[0];
                this.credits = (e = n.resources) == null ? void 0 : e.exportCredits
            } catch (t) {
                bt(t, this)
            }
        }
    }
}
