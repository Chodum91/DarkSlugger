        biggestMedia() {
            if (!this.item.media)
                return {
                    width: this.width,
                    height: this.height
                };
            const e = pw(this.item.media, this.itemType);
            return {
                width: parseFloat(e.width),
                height: parseFloat(e.height)
            }
        },
        mediaStyles() {
            const {height: e, width: t} = JR(this.biggestMedia, {
                height: this.containerHeight,
                width: this.containerWidth
            });
            return {
                height: `${e}px`,
                width: `${t}px`
            }
        },
        itemType() {
            return this.forceItemType || cr(this.item)
        }
    },
    methods: {
        setVideoDimensions({height: e, width: t}) {
            this.height = e,
            this.width = t
        }
    }
