}
  , JC = "/api/rest/contact"
  , OH = (e={})=>Qt(`${JC}/getList`, e)
  , PH = ({email: e, name: t, description: n=null})=>Ya(`${JC}/set`, {
    id: 0,
    email: e,
    name: t,
    description: n
})
  , AH = "/api/rest/group"
  , DH = async(e={})=>Qt(`${AH}/getList`, e)
  , BH = {
    components: {
        SeparatorBar: Pg,
        CollapsibleSection: Ag,
        DialogFooterBar: _i,
        CancelButton: Kn,
        ContinueButton: I9,
        SaveButton: Os,
        SearchInput: Dg,
        NoResultsText: YC,
        SharePermissionsDialog: kH,
        ShareList: bH,
        ContactShareList: xH,
        GroupShareList: IH,
        SharedWithList: fH,
        AddContactDialog: F9,
        ErrorDialog: V9,
        SharedWithWarningDialog: K9,
        ContactShareWarningDialog: Y9,
        RaisedButton: tr
    },
    props: {
        thread: {
            type: Object,
            required: !0,
            validator: e=>$t(["id"], e)
        },
        currentUser: {
            type: Object,
            required: !0,
            validator: e=>$t(["id", "name"], e)
        },
        isAuthor: {
            type: Boolean,
            required: !0
        },
        permissionsOptions: {
            type: Array,
            required: !0
        }
    },
    data: ()=>({
        contacts: {
            available: []
        },
        groups: {
            available: []
        },
        courses: {
            available: []
        },
        items: {
            selected: [],
            updated: []
        },
        modals: {
            selectSharePermissions: !1,
            addContact: !1,
            addContactError: !1,
            sharedWithWarning: !1,
            contactShareWarning: !1
        },
        addContactErrorInformation: {
            title: "",
            message: ""
        },
        permissions: {},
        search: {
            term: "",
            results: [],
            showResults: !1
        },
        isLoading: {
            sharePermissionsDialog: !1,
            updateSharedWithItems: !1
        }
    }),
    computed: {
        sortedContacts() {
            const e = [...this.contacts.available];
            return this.sortArrayByString(e, "name")
        },
        sortedGroups() {
            return this.sortArrayByString(this.groups.available, "name")
        },
        sortedCourses() {
            return this.sortArrayByString(this.courses.available, "name")
        },
        processedPermissions() {
            if (!this.permissions)
                return {};
            const e = {};
            return Object.keys(this.permissions).forEach(t=>{
                e[t] = {},
                this.permissions[t].forEach(n=>{
                    e[t][n.id] = n.level
                }
                )
            }
            ),
            e
        },
        sharedWithPermissions() {
            if (!Object.keys(this.permissions).length || !this.courses.available.length && !this.groups.available.length)
                return [];
            const e = [];
            if (this.permissions.users) {
                const t = this.permissions.users.map(n=>ve(V({}, n), {
                    userId: Number(n.id),
                    type: Kr
                }));
                e.push(...t)
            }
            if (this.permissions.groups) {
                const t = this.permissions.groups.map(n=>ve(V({}, n), {
                    id: Number(n.id),
                    type: [...this.courses.available, ...this.groups.available].find(r=>r.id === Number(n.id)).type
                }));
                e.push(...t)
            }
            return e
        },
        sharedWithItems() {
            return this.sortArrayByString(this.sharedWithPermissions, "name")
        },
        staticItems() {
            const e = []
              , t = {
                userId: this.thread.userId,
                avatarUrl: this.thread.authorAvatarUrl,
                name: this.thread.authorName,
                type: Kr
            };
            if (e.push(t),
            !this.isAuthor) {
                const n = ve(V({}, this.currentUser), {
                    type: Kr
                });
                e.push(n)
            }
            return e
        },
        itemHasPermissions() {
            for (const e of this.items.selected)
                if (e.type === El || e.type === Il) {
                    if (this.sharedWithItems.find(t=>t.id === e.id))
                        return !0
                } else if (this.sharedWithItems.find(t=>t.userId === e.userId))
                    return !0;
            return !1
        }
    },
    async created() {
        this.updateContacts(),
        this.updateGroupsAndCourses(),
        this.updateThreadPermissions()
    },
    methods: {
        sortArrayByString(e, t, n=!0) {
            return e.sort((r,a)=>(r[t].toLowerCase() < a[t].toLowerCase() ? -1 : 1) * (n ? 1 : -1))
        },
        doSearch(e) {
            this.search.term = e;
            const t = ["name", "email", "userName"]
              , r = [...this.contacts.available, ...this.groups.available, ...this.courses.available].filter(a=>t.some(s=>a[s] !== void 0 && a[s].toLowerCase().includes(e.toLowerCase())));
            if (e === "") {
                this.clearSearch();
                return
            }
            this.search.showResults = !0,
            this.search.results = r
        },
        clearSearch() {
            this.search.showResults = !1,
            this.search.results = []
        },
        addToUpdatedList(e) {
            if (this.items.selected.length) {
                this.modals.contactShareWarning = !0;
                return
            }
            const t = this.items.updated.find(n=>n.itemId === e.itemId && n.type === e.type);
            if (t) {
                t.permission = e.permission;
                return
            }
            this.items.updated.push(e)
        },
        async updateSharedWithItems() {
            if (!this.items.updated.length)
                return;
            this.isLoading.updateSharedWithItems = !0;
            const e = this.findPermissionLevelsToBeChanged();
            for (const t of e) {
                const n = []
                  , r = [];
                for (const a of this.items.updated)
                    a.permission === t && (a.type === Kr && n.push(a.itemId),
                    (a.type === El || a.type === Il) && r.push(a.itemId));
                await this.grantPermissionsByLevel(t, n, r)
            }
            this.showSuccessNotification(),
            this.clearSelection(),
            this.isLoading.updateSharedWithItems = !1,
            this.modals.sharedWithWarning = !1
        },
        findPermissionLevelsToBeChanged() {
            return [...new Set(this.items.updated.map(e=>e.permission))]
        },
        async grantPermissionsByLevel(e, t, n) {
            try {
                const {data: r} = await Mb(this.thread.id, {
                    groupId: n,
                    userId: t,
                    level: e
                });
                if (typeof r != "number") {
                    zn(this);
                    return
                }
                await this.updateThreadPermissions()
            } catch (r) {
                bt(r)
            }
        },
        clearSelection() {
            this.items.selected = [],
            this.items.updated = []
        },
        handleItemSelect(e) {
            if (this.items.updated.length) {
                this.modals.sharedWithWarning = !0;
                return
            }
            const t = e.userId ? "userId" : "id"
              , n = this.items.selected.findIndex(r=>r[t] === e[t] && r.type === e.type);
            n !== -1 ? this.items.selected.splice(n, 1) : this.items.selected.push(e)
        },
        async grantPermissionsToSelected({permission: e, notifyByEmail: t}) {
            if (!this.items.selected.length)
                return;
            this.isLoading.sharePermissionsDialog = !0;
            const n = this.items.selected.filter(a=>a.type !== Kr)
              , r = this.items.selected.filter(a=>a.type === Kr);
            try {
                const {data: a} = await Mb(this.thread.id, {
                    groupId: n.map(s=>s.id),
                    userId: r.map(s=>s.userId),
                    level: e,
                    sendEmail: t
                });
                if (typeof a != "number") {
                    zn(this);
                    return
                }
                await this.updateThreadPermissions(),
                this.showSuccessNotification()
            } catch (a) {
                bt(a)
            } finally {
                this.clearSelection(),
                this.modals.selectSharePermissions = !1,
                this.isLoading.sharePermissionsDialog = !1
            }
        },
        async updateThreadPermissions() {
            const {data: e} = await zD({
                id: this.thread.id,
                full: !0
            });
            this.permissions = e
        },
        async updateContacts() {
            const {data: e} = await OH();
            this.contacts.available = e.map(t=>ve(V({}, t), {
                type: Kr
            }))
        },
        async addNewContact({name: e, email: t}) {
            this.modals.addContact = !1;
            try {
                await PH({
                    name: e,
                    email: t
                }),
                await this.updateContacts(),
                this.$notify({
                    title: this.translate("labels.success"),
                    text: this.translate("toasts.contactAdded"),
                    type: "success"
                })
            } catch (n) {
                n.response && n.response.status === 403 ? (this.addContactErrorInformation = {
                    title: n.response.statusText,
                    message: n.response.data
                },
                this.modals.addContactError = !0) : bt(n, this)
            }
        },
        async updateGroupsAndCourses() {
            const {data: e} = await DH();
            this.groups.available = e.filter(t=>t.type === El),
            this.courses.available = e.filter(t=>t.type === Il)
        },
        showSuccessNotification() {
            jn()
        }
    }
}
  , RH = {
    class: "h-full"
}
  , MH = {
    class: "mx-4 mb-8"
}
  , LH = {
    class: "mb-20 lg:mx-4"
}
  , FH = {
    class: "flex flex-col"
}
  , NH = {
