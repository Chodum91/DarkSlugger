var $7 = Z(C7, [["render", S7]]);
const x7 = {
    name: "TimeInput",
    props: {
        label: {
            type: String,
            required: !0
        },
        totalSeconds: {
            type: Number,
            required: !0
        },
        hasError: {
            type: Boolean,
            default: !1
        },
        errorText: {
            type: String,
            default: null
        },
        max: {
            type: Number,
            default: 3599
        }
    },
    emits: ["update:totalSeconds"],
    setup() {
        const {uuid: e} = on();
        return {
            uuid: e
        }
    },
    data: ()=>({
        minutes: 0,
        seconds: 0
    }),
    computed: {
        labelId() {
            return "label-" + this.uuid
        },
        errorTextId() {
            return "error-" + this.uuid
        },
        describedByIds() {
            const e = [this.labelId];
            return this.hasError && this.errorText && e.push(this.errorTextId),
            e.join(" ")
        },
        maxMinutes() {
            return this.calcMinutes(this.max)
        },
        maxSeconds() {
            return this.minutes === this.maxMinutes ? this.calcSeconds(this.max) : 59
        }
    },
    watch: {
        totalSeconds: {
            immediate: !0,
            handler(e) {
                this.minutes = this.calcMinutes(e) || 0,
                this.seconds = this.calcSeconds(e) || 0
            }
        },
        minutes(e) {
            e > this.maxMinutes && (this.minutes = this.maxMinutes),
            this.emitSeconds()
        },
        seconds(e) {
            e > this.maxSeconds && (this.seconds = this.maxSeconds),
            this.emitSeconds()
        }
    },
    methods: {
        calcMinutes(e) {
            return Math.floor(e / 60)
        },
        calcSeconds(e) {
            return e % 60
        },
        emitSeconds() {
            this.$emit("update:totalSeconds", this.seconds + this.minutes * 60)
        }
    }
}
