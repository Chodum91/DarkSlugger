            mutationId: ++this.mutationId,
            options: t.defaultMutationOptions(n),
            state: r,
            defaultOptions: n.mutationKey ? t.getMutationDefaults(n.mutationKey) : void 0,
            meta: n.meta
        });
        return this.add(a),
        a
    }
    add(t) {
        this.mutations.push(t),
        this.notify({
            type: "added",
            mutation: t
        })
    }
    remove(t) {
        this.mutations = this.mutations.filter(n=>n !== t),
        this.notify({
            type: "removed",
