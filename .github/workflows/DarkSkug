class mw {
    constructor() {
        this._events = {}
    }
    on(t, n) {
        this._events[t] = [n]
    }
    off(t) {
        t in this._events && delete this._events[t]
    }
    trigger(t, n=null) {
        if (t in this._events)
            for (const r of this._events[t])
                r(this, n)
    }
}
