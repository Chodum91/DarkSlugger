        this.mutations.find(n=>g0(t, n))
    }
    findAll(t) {
        return this.mutations.filter(n=>g0(t, n))
    }
    notify(t) {
        qn.batch(()=>{
            this.listeners.forEach(n=>{
                n(t)
            }
            )
        }
        )
    }
