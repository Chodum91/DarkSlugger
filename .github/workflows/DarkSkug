class P5 {
    constructor() {
        this.storage = new Map
    }
    set(t, n, r) {
        this.storage.set(t, {
            rules: n,
            result: r
        })
    }
    checkRulesValidity(t, n, r) {
        const a = Object.keys(r)
          , s = Object.keys(n);
        return s.length !== a.length || !s.every(u=>a.includes(u)) ? !1 : s.every(u=>n[u].$params ? Object.keys(n[u].$params).every(c=>_(r[u].$params[c]) === _(n[u].$params[c])) : !0)
    }
    get(t, n) {
        const r = this.storage.get(t);
        if (!r)
            return;
        const {rules: a, result: s} = r
          , l = this.checkRulesValidity(t, n, a)
          , u = s.$unwatch ? s.$unwatch : ()=>({});
        return l ? s : {
            $dirty: s.$dirty,
            $partial: !0,
            $unwatch: u
        }
    }
