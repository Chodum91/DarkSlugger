var D7 = Z(O7, [["render", A7]]);
const B7 = {
    components: {
        TimeInput: ZC
    },
    props: {
        duration: {
            type: Number,
            default: 0
        }
    },
    emits: ["update"],
    setup() {
        const {uuid: e} = on();
        return {
            uuid: e
        }
    },
    data() {
        return {
            selectedValue: ""
        }
    },
    computed: {
        hideInputs() {
            return this.duration === 0
        }
    },
    created() {
        if (this.duration !== 0) {
            if (this.duration === 15) {
                this.selectedValue = "ignite";
                return
            }
            if (this.duration === 20) {
                this.selectedValue = "pecha-kucha";
                return
            }
            this.selectedValue = "custom"
        }
    },
    methods: {
        toggleInputs() {
            if (!this.hideInputs) {
                this.updateDuration(0),
                this.selectedValue = "";
                return
            }
            this.selectedValue = "custom",
            this.updateDuration(20)
        },
        updateDuration(e) {
            this.$emit("update", e)
        },
        updateSelectedValue(e) {
            e === "pecha-kucha" && this.updateDuration(20),
            e === "ignite" && this.updateDuration(15),
            this.selectedValue = e
        }
    }
}
