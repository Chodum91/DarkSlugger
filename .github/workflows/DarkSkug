var pJ = Z(uJ, [["render", fJ]]);
const hJ = {
    components: {
        FlatButton: nt,
        CopyThreadCard: pJ,
        CopyCommentsDropdown: kS,
        A11yAnnouncer: vo
    },
    props: {
        thread: {
            type: Object,
            required: !0,
            validator(e) {
                return "id"in e
            }
        }
    },
    emits: ["selected:pages"],
    data: ()=>({
        pages: null,
        overrideComments: !1,
        commentsOverride: {},
        threadCopyOptions: [],
        selectedPageIds: []
    }),
    computed: {
        selectedPageIndexes() {
            return this.selectedPageIds.map(t=>this.pageIndexForId(t)).filter(t=>t !== -1)
        }
    },
    watch: {
        thread: {
            immediate: !0,
            deep: !0,
            handler() {
                this.loadPages()
            }
        },
        selectedPageIds: {
            deep: !0,
            handler() {
                this.announceUserSelection()
            }
        },
        threadCopyOptions: {
            deep: !0,
            handler() {
                !this.selectedPageIds.length || this.announceUserSelection()
            }
        }
    },
    methods: {
        async loadPages() {
            const {data: e} = await vg(this.thread.id, {
                properties: ["myCommentCount", "commentCount"]
            });
            this.pages = e,
            this.threadCopyOptions = [],
            this.pages.forEach((t,n)=>{
                this.threadCopyOptions[n] = this.overrideComments ? this.commentsOverride : bC()
            }
            )
        },
        biggestImage(e) {
            return ru(e.media).url
        },
        updateCopyOption(e, t, n=!1) {
            this.overrideComments && n && (this.overrideComments = !1),
            this.threadCopyOptions[e] = t
        },
        pageIndexForId(e) {
            return this.pages.findIndex(t=>t.id === e)
        },
        togglePageSelection(e) {
            const t = this.selectedPageIds.findIndex(n=>n === e.id);
            if (t === -1) {
                this.selectedPageIds.push(e.id),
                this.$refs.announcer.announce(this.translate("messages.pageSelected", {
                    slideTitle: e.title || this.translate("placeholders.untitled")
                }));
                return
            }
            this.selectedPageIds.splice(t, 1),
            this.$refs.announcer.announce(this.translate("messages.pageUnselected", {
                slideTitle: e.title || this.translate("placeholders.untitled")
            }))
        },
        applyCommentOverride() {
            for (const e in this.pages)
                this.updateCopyOption(e, this.commentsOverride)
        },
        toggleCommentsOverride(e) {
            this.overrideComments = e,
            this.overrideComments && this.applyCommentOverride()
        },
        updateCommentsOverride(e) {
            this.commentsOverride = e,
            this.overrideComments && this.applyCommentOverride()
        },
        selectAll() {
            this.selectedPageIds = [],
            this.pages.forEach(e=>{
                this.selectedPageIds.push(e.id)
            }
            ),
            this.$refs.announcer.announce(yr("messages.pagesSelected", this.pages.length))
        },
        deselectAll() {
            this.selectedPageIds = [],
            this.$refs.announcer.announce(this.translate("messages.allPagesUnselected"))
        },
        announceUserSelection() {
            const e = this.selectedPageIndexes.map(t=>({
                page: this.pages[t],
                copy: this.threadCopyOptions[t]
            }));
            this.$emit("selected:pages", e)
        }
    }
}
  , mJ = {
    class: "mb-4 font-bold"
}
  , gJ = {
    class: "flex flex-wrap items-center justify-between lg:flex-nowrap"
}
  , vJ = {
    class: "flex flex-wrap items-center md:flex-nowrap"
}
  , yJ = {
    class: "grid grid-cols-1 gap-2 md:grid-cols-2 lg:grid-cols-3 xl:grid-col-5"
};
