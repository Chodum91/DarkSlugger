var u7 = Z(o7, [["render", l7]]);
const c7 = {
    components: {
        A11yAnnouncer: vo,
        AspectRatioDropdown: i7,
        LockIconButton: u7
    },
    props: {
        ratioTypes: {
            type: Array,
            default: ()=>[],
            validator(e) {
                return e.every(t=>Rc().map(n=>n.type).indexOf(t) !== -1)
            }
        }
    },
    emits: ["select:ratio"],
    data: ()=>({
        aspectRatio: {},
        width: 0,
        height: 0
    }),
    computed: {
        boxDisplaySizes() {
            let e, t;
            switch (this.aspectRatio.type) {
            case vl:
                e = 109,
                t = 192;
                break;
            case zm:
                e = 109,
                t = 192;
                break;
            case Km:
                e = 109,
                t = 146;
                break;
            case Gm:
                t = 109,
                e = 141
            }
            return {
                height: `${e}px`,
                width: `${t}px`
            }
        },
        isNonCustomHeight() {
            return this.aspectRatio.type !== Qi && this.aspectRatio.type !== vl
        },
        unitsWidthText() {
            return this.aspectRatio.type === vl ? "labels.percentWidth" : "labels.pxWidth"
        },
        ratioOptions() {
            return this.ratioTypes.length ? Rc().filter(e=>this.ratioTypes.includes(e.type)) : Rc()
        },
        lockLabel() {
            const e = this.ratioOptions;
            return this.aspectRatio.type === Qi ? this.translate("actions.setAspectRatioToPredefined", {
                aspectratio: e[0].label
            }) : this.translate("actions.setAspectRatioToCustom")
        }
    },
    watch: {
        height() {
            this.emitSizes()
        },
        width() {
            this.emitSizes()
        }
    },
    methods: {
        setAspectRatio(e) {
            this.aspectRatio = e,
            e.width && (this.width = e.width),
            e.height && (this.height = e.height)
        },
        lockClick() {
            const e = this.ratioOptions;
            this.aspectRatio.type === Qi ? this.setAspectRatio(e[0]) : this.setAspectRatio(e.find(t=>t.type === Qi)),
            this.$refs.announcer.announce(this.translate("messages.embedAspectRatioUpdated", {
                aspectratio: this.aspectRatio.label
            }))
        },
        emitSizes() {
            this.$emit("select:ratio", {
                height: parseInt(this.height),
                width: this.aspectRatio.type === vl ? `${this.aspectRatio.width}%` : parseInt(this.width)
            })
        }
    }
}
