const O7 = {
    components: {
        TimeInput: ZC
    },
    props: {
        duration: {
            type: Number,
            default: 3600
        }
    },
    emits: ["update"],
    setup() {
        const {uuid: e} = on();
        return {
            uuid: e
        }
    },
    computed: {
        disableInputs() {
            return this.duration >= 3600
        }
    },
    methods: {
        toggleInputs() {
            if (this.duration < 3600) {
                this.updateDuration(3600);
                return
            }
            this.updateDuration(3599)
        },
        updateDuration(e) {
            this.$emit("update", e)
        }
    }
