        return (r = this.queryCache.find(t, n)) == null ? void 0 : r.state
    }
    removeQueries(t, n) {
        const [r] = ys(t, n)
          , a = this.queryCache;
        qn.batch(()=>{
            a.findAll(r).forEach(s=>{
                a.remove(s)
            }
            )
        }
        )
    }
    resetQueries(t, n, r) {
        const [a,s] = ys(t, n, r)
          , l = this.queryCache
          , u = V({
            type: "active"
        }, a);
        return qn.batch(()=>(l.findAll(a).forEach(c=>{
            c.reset()
        }
        ),
        this.refetchQueries(u, s)))
    }
    cancelQueries(t, n, r) {
        const [a,s={}] = ys(t, n, r);
        typeof s.revert == "undefined" && (s.revert = !0);
        const l = qn.batch(()=>this.queryCache.findAll(a).map(u=>u.cancel(s)));
        return Promise.all(l).then(zr).catch(zr)
    }
    invalidateQueries(t, n, r) {
        const [a,s] = ys(t, n, r);
        return qn.batch(()=>{
            var l, u;
            if (this.queryCache.findAll(a).forEach(d=>{
                d.invalidate()
            }
            ),
            a.refetchType === "none")
                return Promise.resolve();
            const c = ve(V({}, a), {
                type: (l = (u = a.refetchType) != null ? u : a.type) != null ? l : "active"
            });
            return this.refetchQueries(c, s)
        }
        )
    }
    refetchQueries(t, n, r) {
        const [a,s] = ys(t, n, r)
          , l = qn.batch(()=>this.queryCache.findAll(a).filter(c=>!c.isDisabled()).map(c=>{
            var d;
            return c.fetch(void 0, ve(V({}, s), {
                cancelRefetch: (d = s == null ? void 0 : s.cancelRefetch) != null ? d : !0,
                meta: {
                    refetchPage: a.refetchPage
                }
            }))
        }
        ));
        let u = Promise.all(l).then(zr);
        return s != null && s.throwOnError || (u = u.catch(zr)),
        u
    }
    fetchQuery(t, n, r) {
        const a = zp(t, n, r)
          , s = this.defaultQueryOptions(a);
        typeof s.retry == "undefined" && (s.retry = !1);
        const l = this.queryCache.build(this, s);
        return l.isStaleByTime(s.staleTime) ? l.fetch(s) : Promise.resolve(l.state.data)
    }
    prefetchQuery(t, n, r) {
        return this.fetchQuery(t, n, r).then(zr).catch(zr)
    }
    fetchInfiniteQuery(t, n, r) {
        const a = zp(t, n, r);
        return a.behavior = Vh(),
        this.fetchQuery(a)
    }
    prefetchInfiniteQuery(t, n, r) {
        return this.fetchInfiniteQuery(t, n, r).then(zr).catch(zr)
    }
    resumePausedMutations() {
        return this.mutationCache.resumePausedMutations()
    }
    getQueryCache() {
        return this.queryCache
    }
    getMutationCache() {
        return this.mutationCache
    }
    getLogger() {
        return this.logger
    }
    getDefaultOptions() {
        return this.defaultOptions
    }
    setDefaultOptions(t) {
        this.defaultOptions = t
    }
    setQueryDefaults(t, n) {
        const r = this.queryDefaults.find(a=>ii(t) === ii(a.queryKey));
        r ? r.defaultOptions = n : this.queryDefaults.push({
            queryKey: t,
            defaultOptions: n
        })
    }
    getQueryDefaults(t) {
        if (!t)
            return;
        const n = this.queryDefaults.find(r=>Yc(t, r.queryKey));
        return n == null ? void 0 : n.defaultOptions
    }
    setMutationDefaults(t, n) {
        const r = this.mutationDefaults.find(a=>ii(t) === ii(a.mutationKey));
        r ? r.defaultOptions = n : this.mutationDefaults.push({
            mutationKey: t,
            defaultOptions: n
        })
    }
    getMutationDefaults(t) {
        if (!t)
            return;
        const n = this.mutationDefaults.find(r=>Yc(t, r.mutationKey));
        return n == null ? void 0 : n.defaultOptions
    }
    defaultQueryOptions(t) {
        if (t != null && t._defaulted)
            return t;
        const n = ve(V(V(V({}, this.defaultOptions.queries), this.getQueryDefaults(t == null ? void 0 : t.queryKey)), t), {
            _defaulted: !0
        });
        return !n.queryHash && n.queryKey && (n.queryHash = ng(n.queryKey, n)),
        typeof n.refetchOnReconnect == "undefined" && (n.refetchOnReconnect = n.networkMode !== "always"),
        typeof n.useErrorBoundary == "undefined" && (n.useErrorBoundary = !!n.suspense),
        n
    }
    defaultMutationOptions(t) {
        return t != null && t._defaulted ? t : ve(V(V(V({}, this.defaultOptions.mutations), this.getMutationDefaults(t == null ? void 0 : t.mutationKey)), t), {
            _defaulted: !0
        })
    }
    clear() {
        this.queryCache.clear(),
        this.mutationCache.clear()
    }
}
class Rw extends ho {
    constructor(t, n) {
        super();
        this.client = t,
        this.options = n,
        this.trackedProps = new Set,
        this.selectError = null,
        this.bindMethods(),
        this.setOptions(n)
    }
    bindMethods() {
        this.remove = this.remove.bind(this),
        this.refetch = this.refetch.bind(this)
    }
    onSubscribe() {
        this.listeners.length === 1 && (this.currentQuery.addObserver(this),
        _0(this.currentQuery, this.options) && this.executeFetch(),
        this.updateTimers())
    }
    onUnsubscribe() {
        this.listeners.length || this.destroy()
    }
    shouldFetchOnReconnect() {
        return jh(this.currentQuery, this.options, this.options.refetchOnReconnect)
    }
    shouldFetchOnWindowFocus() {
        return jh(this.currentQuery, this.options, this.options.refetchOnWindowFocus)
    }
    destroy() {
        this.listeners = [],
        this.clearStaleTimeout(),
        this.clearRefetchInterval(),
        this.currentQuery.removeObserver(this)
    }
    setOptions(t, n) {
        const r = this.options
          , a = this.currentQuery;
        if (this.options = this.client.defaultQueryOptions(t),
        Fh(r, this.options) || this.client.getQueryCache().notify({
            type: "observerOptionsUpdated",
            query: this.currentQuery,
            observer: this
        }),
        typeof this.options.enabled != "undefined" && typeof this.options.enabled != "boolean")
            throw new Error("Expected enabled to be a boolean");
        this.options.queryKey || (this.options.queryKey = r.queryKey),
        this.updateQuery();
        const s = this.hasListeners();
        s && w0(this.currentQuery, a, this.options, r) && this.executeFetch(),
        this.updateResult(n),
        s && (this.currentQuery !== a || this.options.enabled !== r.enabled || this.options.staleTime !== r.staleTime) && this.updateStaleTimeout();
