    props: {
        modelValue: {
            type: [String, Number, Object],
            default: null,
            validator(e) {
                return ["string", "number"].includes(typeof e) || !e ? !0 : "value"in e
            }
        },
        options: {
            type: Array,
            required: !0,
            validator(e) {
                return e.find(t=>$t(["label", "value"], t))
            }
        },
        hasError: {
            type: Boolean,
            default: !1
        },
        errorText: {
            type: String,
            default: ""
        },
        label: {
            type: String,
            default: ""
        },
        hideLabel: {
            type: Boolean,
            default: !1
        },
        hideBackground: {
            type: Boolean,
            default: !1
        }
    },
    emits: ["update:modelValue"],
    setup() {
        const {uuid: e} = on();
        return {
            uuid: e
        }
    },
    computed: {
        id() {
            return "baseselect_" + this.uuid
        },
        componentClasses() {
            const e = ["hover:bg-blue-100", "active:bg-blue-100"];
            return this.modelValue && e.push("is-dirty"),
            this.hasError && e.push("bg-orange-100"),
            this.hideBackground || e.push("bg-grey-100"),
            e
        },
        selectValue() {
            return ["string", "number"].includes(typeof this.modelValue) ? this.modelValue : this.modelValue ? this.modelValue.value : null
        }
    },
    methods: {
        handleChange(e) {
            const t = e.target.value;
            if (typeof this.modelValue != "object") {
                this.$emit("update:modelValue", t);
                return
            }
            const n = this.options.find(r=>r.value.toString() === t.toString());
            this.$emit("update:modelValue", n)
        },
        focus() {
            this.$refs.select.focus()
        }
    }
}
