var XQ = Z(KQ, [["render", YQ]]);
const JQ = {
    components: {
        ItemDisplayModule: lQ,
        ItemDisplayMixedCategoryItem: XQ,
        ItemDisplayCategoryGroup: _S,
        ItemDisplayItemGroup: xS
    },
    props: {
        sources: {
            type: Array,
            required: !0
        },
        disabled: {
            type: Boolean,
            default: !1
        },
        selectedItems: {
            type: Array,
            default: ()=>[]
        }
    },
    emits: ["select", "requestPreview"],
    computed: {
        listType() {
            if (!this.sources.length)
                return null;
            const e = new Set;
            return this.sources.forEach(t=>{
                e.add(t.type)
            }
            ),
            e.has("module") ? "module" : e.has("category") && e.has("item") ? "mixed" : e.has("category") ? "category" : "item"
        },
        isMixedView() {
            return this.listType === "mixed"
        },
        categories() {
            return this.sources.filter(e=>e.type === "category")
        },
        items() {
            return this.sources.filter(e=>e.type === "item")
        }
    },
    methods: {
        chooseSource(e) {
            this.$emit("select", e)
        },
        menuItemSelected(e) {
            e.item.label.toLowerCase() === "preview" && this.$emit("requestPreview", e.source)
        },
        focusCardAtIndex(e) {
            this.listType === "item" && this.$refs.itemDisplayGroup.focusCardAtIndex(e)
        }
    }
}
