            const e = [];
            return this.searchFilters.length && e.push({
                title: this.translate("search.filterBy"),
                items: this.searchFilters,
                singleChoice: !0
            }),
            this.sortFilters.length && e.push({
                title: this.translate("search.sortOrder"),
                items: this.sortFilters,
                singleChoice: !0
            }),
            e
        }
    }
