        const t = this.mutations.filter(n=>n.state.isPaused);
        return qn.batch(()=>t.reduce((n,r)=>n.then(()=>r.continue().catch(zr)), Promise.resolve()))
    }
}
function Vh() {
    return {
        onFetch: e=>{
            e.fetchFn = ()=>{
                var t, n, r, a, s, l;
                const u = (t = e.fetchOptions) == null || (n = t.meta) == null ? void 0 : n.refetchPage
                  , c = (r = e.fetchOptions) == null || (a = r.meta) == null ? void 0 : a.fetchMore
                  , d = c == null ? void 0 : c.pageParam
                  , p = (c == null ? void 0 : c.direction) === "forward"
                  , h = (c == null ? void 0 : c.direction) === "backward"
                  , v = ((s = e.state.data) == null ? void 0 : s.pages) || []
                  , b = ((l = e.state.data) == null ? void 0 : l.pageParams) || [];
                let w = b
                  , $ = !1;
                const P = oe=>{
                    Object.defineProperty(oe, "signal", {
                        enumerable: !0,
                        get: ()=>{
                            var J;
                            if ((J = e.signal) != null && J.aborted)
                                $ = !0;
                            else {
                                var Q;
                                (Q = e.signal) == null || Q.addEventListener("abort", ()=>{
                                    $ = !0
                                }
                                )
                            }
                            return e.signal
                        }
                    })
                }
                  , A = e.options.queryFn || (()=>Promise.reject("Missing queryFn"))
                  , X = (oe,J,Q,le)=>(w = le ? [J, ...w] : [...w, J],
                le ? [Q, ...oe] : [...oe, Q])
                  , q = (oe,J,Q,le)=>{
                    if ($)
                        return Promise.reject("Cancelled");
                    if (typeof Q == "undefined" && !J && oe.length)
                        return Promise.resolve(oe);
                    const Ae = {
                        queryKey: e.queryKey,
                        pageParam: Q,
                        meta: e.meta
                    };
                    P(Ae);
                    const je = A(Ae);
                    return Promise.resolve(je).then(We=>X(oe, Q, We, le))
                }
                ;
                let K;
                if (!v.length)
                    K = q([]);
                else if (p) {
                    const oe = typeof d != "undefined"
                      , J = oe ? d : qh(e.options, v);
                    K = q(v, oe, J)
                } else if (h) {
                    const oe = typeof d != "undefined"
                      , J = oe ? d : Bw(e.options, v);
                    K = q(v, oe, J, !0)
                } else {
                    w = [];
                    const oe = typeof e.options.getNextPageParam == "undefined";
                    K = (u && v[0] ? u(v[0], 0, v) : !0) ? q([], oe, b[0]) : Promise.resolve(X([], b[0], v[0]));
                    for (let Q = 1; Q < v.length; Q++)
                        K = K.then(le=>{
                            if (u && v[Q] ? u(v[Q], Q, v) : !0) {
                                const je = oe ? b[Q] : qh(e.options, le);
                                return q(le, oe, je)
                            }
                            return Promise.resolve(X(le, b[Q], v[Q]))
                        }
                        )
                }
                return K.then(oe=>({
                    pages: oe,
                    pageParams: w
                }))
            }
        }
    }
}
function qh(e, t) {
    return e.getNextPageParam == null ? void 0 : e.getNextPageParam(t[t.length - 1], t)
