const oH = {
    name: "ShareListItemWithPermissionsSelect",
    components: {
        ShareListItem: Xd
    },
    props: {
        item: {
            type: Object,
            required: !0
        },
        permissionsOptions: {
            type: Array,
            required: !0
        },
        permissionLevel: {
            type: Number,
            required: !0
        },
        selectDisabled: {
            type: Boolean,
            default: !1
        }
    },
    emits: ["update:item", "update:disabled"],
    computed: {
        permissionVerb() {
            return this.permissionsOptions.find(e=>e.numeric === parseInt(this.permissionLevel)).verb
        },
        permissionValue() {
            return this.permissionsOptions.find(e=>e.numeric === parseInt(this.permissionLevel)).value
        }
    },
    created() {
        this.options = this.threadOptionsWithVerb(this.permissionsOptions)
    },
    methods: {
        threadOptionsWithVerb(e) {
            return e.map(t=>ve(V({}, t), {
                label: t.verb
            }))
        },
        selectPermission(e) {
            this.$emit("update:item", {
                permission: e,
                itemId: this.item.userId || this.item.id,
                type: this.item.type
            })
        }
    }
};
