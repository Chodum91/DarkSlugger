var n8 = Z(ZU, [["render", t8]]);
const r8 = {
    props: {
        angle: {
            type: Number,
            default: 0,
            validator(e) {
                return [0, 90, 180, 270].includes(e)
            }
        },
        maxHeight: {
            type: Number,
            default: null
        }
    },
    data() {
        return {
            wrapperHeight: 0,
            wrapperWidth: 0,
            imageHeight: 0,
            imageWidth: 0
        }
    },
    computed: {
        imageRotation() {
            return this.angle === 0 ? null : {
                transform: `rotate(${this.angle}deg)`
            }
        },
        imageWrapperSizes() {
            return this.angle === 90 || this.angle === 270 ? {
                width: `${this.wrapperHeight}px`
            } : null
        },
        imageClasses() {
            return [0, 180].includes(this.angle) ? ["h-full"] : []
        }
    },
    watch: {
        angle() {
            this.calculateWrapperDimensions()
        }
    },
    mounted() {
        this.calculateWrapperDimensions(),
        this.calculateImageDimensions()
    },
    methods: {
        calculateWrapperDimensions() {
            const t = this.$el.getBoundingClientRect();
            this.wrapperHeight = this.maxHeight && t.height > this.maxHeight ? this.maxHeight : t.height,
            this.wrapperWidth = t.width
        },
        calculateImageDimensions() {
            const t = this.$el.querySelector("img").getBoundingClientRect();
            this.imageHeight = t.height,
            this.imageWidth = t.width
        }
    }
};
function a8(e, t, n, r, a, s) {
    return m(),
    M("div", {
        class: "flex justify-center",
