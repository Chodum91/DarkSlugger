    role: "status",
    class: "vt-sr-only"
}
  , UH = {
    class: "relative lg:flex"
}
  , VH = {
    key: 1,
    class: "mb-6 text-blue-900 lg:mb-8 lg:ml-6"
}
  , qH = {
    class: "absolute top-0 right-0 lg:static"
}
  , jH = {
    key: 1,
    class: "mb-6 text-blue-900 lg:mb-8 lg:ml-6"
}
  , HH = {
    key: 1,
    class: "mb-6 text-blue-900 lg:mb-8 lg:ml-6"
}
  , WH = {
    key: 0,
    class: "font-bold text-orange-700"
}
  , zH = {
    class: "ml-auto"
};
function KH(e, t, n, r, a, s) {
    const l = S("SearchInput")
      , u = S("NoResultsText")
      , c = S("ShareList")
      , d = S("SeparatorBar")
      , p = S("CollapsibleSection")
      , h = S("ContactShareList")
      , v = S("RaisedButton")
      , b = S("GroupShareList")
      , w = S("SharedWithList")
      , $ = S("CancelButton")
      , P = S("ContinueButton")
      , A = S("SaveButton")
      , X = S("DialogFooterBar")
