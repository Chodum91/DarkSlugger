function iu(e, {emit: t}) {
    return {
        updateValue: r=>{
            r.stopPropagation();
            let a = r.target.value;
            if (r.target.type === "checkbox" && (a = r.target.checked),
            r.target.type === "radio") {
                if (!r.target.checked)
                    return;
                a = e.value
            }
            t("update:modelValue", a)
        }
    }
