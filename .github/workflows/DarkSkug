    computed: {
        commentModerationStatus() {
            const e = this.thread.moderated ? "on" : "off";
            return this.translate("labels.commentModerationStatus", {
                status: e
            })
        }
    },
    watch: {
        thread: {
            immediate: !0,
            deep: !0,
            async handler(e) {
                if ($t(this.threadParams.properties, e))
                    return;
                const {data: t} = await nu(this.thread.id, this.threadParams);
                this.$emit("update:thread", t)
            }
        }
    },
    async created() {
        try {
            const {data: e} = await oW();
            this.setAvailablePlatforms(e)
        } catch (e) {
            bt(e, this)
        }
    },
    methods: {
        setAvailablePlatforms(e) {
            e.forEach(t=>{
                t.id in ud && this.availablePlatforms.push(V({
                    id: t.id
                }, ud[t.id]))
            }
            )
        },
        handleSettingsSaved(e) {
            this.$emit("update:thread", e),
            this.modals.threadSettings = !1
        },
        setPlatformAndShare(e) {
            this.selectedPlatform = e,
            this.openSharingWindow()
        },
        openSharingWindow(e=!1) {
            if (this.linkSharingIsOff && !e) {
                this.modals.linkSharingOff = !0;
                return
            }
            fW(this.selectedPlatform, this.thread)
        },
        updatePrivacyAndShare(e) {
            (e == null ? void 0 : e.type) !== "private" && this.$emit("update:shareMode", e),
            this.modals.linkSharingOff = !1,
            this.openSharingWindow(!0)
        }
    }
}
