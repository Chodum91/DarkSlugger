        "onUpdate:modelValue": s.toggleCommentsOverride
    }, null, 8, ["model-value", "label", "onUpdate:modelValue"]), I(u, {
        class: "flex-none",
        "selected-option": e.commentsOverride,
        "menu-classes": ["left-0 md:left-auto md:right-0"],
        "without-counter": "",
        "force-all-options": "",
        "closed-text-color": "text-blue-900",
        "onUpdate:selectedOption": s.updateCommentsOverride
    }, null, 8, ["selected-option", "onUpdate:selectedOption"])]), O("div", null, [I(c, {
        onClick: s.selectAll
    }, {
        default: x(()=>[ce(L(e.translate("actions.selectAll")), 1)]),
        _: 1
    }, 8, ["onClick"]), I(c, {
        onClick: s.deselectAll
    }, {
        default: x(()=>[ce(L(e.translate("actions.deselectAll")), 1)]),
        _: 1
    }, 8, ["onClick"])])]), O("div", yJ, [(m(!0),
    M(Qe, null, Nt(e.pages, (h,v)=>(m(),
    T(d, {
        key: h.id,
        title: h.title || e.translate("placeholders.untitled"),
        "comments-count": h.commentCount,
        "my-comments-count": h.myCommentCount,
