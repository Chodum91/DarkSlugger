var tS = Z(Q7, [["render", Y7]]);
const X7 = {
    components: {
        AspectRatioSelector: y7,
        RaisedButton: tr,
        FlatButton: nt,
        DialogFooterBar: _i,
        LinkSharingOffDialog: tS,
        ThreadSettingsDialog: eS
    },
    props: {
        linkSharingIsOff: {
            type: Boolean,
            default: !1
        },
        thread: {
            type: Object,
            required: !0,
            validator: e=>$t(["id"], e)
        },
        accessSchema: {
            type: Object,
            required: !0,
            validator: e=>$t(["playbackSettings"], e)
        }
    },
    emits: ["update:thread", "update:shareMode"],
    data: ()=>({
        aspectRatio: {
            width: 0,
            height: 0
        },
        modals: {
            linkSharingOff: !1,
            threadSettings: !1
        },
        threadParams: {
            properties: ["viewCount", "commentCount", "permission", "subscribed", ...og()]
        }
    }),
    computed: {
        embedCode() {
            const e = MD()
              , t = `${e}/app/player/?threadId=${this.thread.id}`;
            return '<iframe width="' + this.aspectRatio.width + '" height="' + this.aspectRatio.height + '" src="' + t + '" frameborder="0" allowusermedia allowfullscreen allow="camera ' + e + "; microphone " + e + "; fullscreen " + e + ';"></iframe>'
        },
        commentModerationStatus() {
            const e = this.thread.moderated ? "on" : "off";
            return this.translate("labels.commentModerationStatus", {
                status: e
            })
        },
        ratioTypes() {
            return [Qi, zm, Km, Gm]
        }
    },
    watch: {
        thread: {
            immediate: !0,
            deep: !0,
            async handler(e) {
                if ($t(this.threadParams.properties, e))
                    return;
                const {data: t} = await nu(this.thread.id, this.threadParams);
                this.$emit("update:thread", t)
            }
        }
    },
    methods: {
        copyEmbedCode(e=!1) {
            if (this.linkSharingIsOff && !e) {
                this.modals.linkSharingOff = !0;
                return
            }
            Tg(this.embedCode),
            this.$notify({
                title: this.translate("labels.success"),
                text: this.translate("toasts.embedCopiedToClipboard"),
                type: "success"
            })
        },
        updatePrivacyAndCopy(e) {
            (e == null ? void 0 : e.type) !== "private" && this.$emit("update:shareMode", e),
            this.modals.linkSharingOff = !1,
            this.copyEmbedCode(!0)
        },
        handleSettingsSaved(e) {
            this.$emit("update:thread", e),
            this.modals.threadSettings = !1
        }
    }
}
