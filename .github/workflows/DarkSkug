    emits: ["close", "media", "tabbablesChanged", "dialogShown", "dialogHidden"],
    setup() {
        const {isNewestModal: e} = Dd();
        return {
            isNewestModal: e
        }
    },
    data() {
        return {
            trapOptions: {
                initialFocus: "#recorder-container"
            }
        }
    },
    ventHandler: new mw,
    ventEvents: {
        "fromswf:close": "close",
        "fromswf:media": "media",
        "fromswf:tabbablesChanged": "tabbablesChanged",
        "fromswf:dialogShown": "dialogShown",
        "fromswf:dialogHidden": "dialogHidden"
    },
    watch: {
        threadId: {
            immediate: !0,
            handler(e) {
                this.$options.ventHandler.trigger("toswf:location:change", {
                    threadId: e
                })
            }
        }
    },
    created() {
        const e = this.$options.ventHandler
          , t = this
          , n = {
            captureMode: t.captureMode,
            maxRecordDuration: t.maxRecordDuration,
            showCloseButton: t.showCloseButton
        };
        this.tabFocusMode && (n.accessibilityMode = "tabFocus"),
        hw().then(()=>{
            window.attachRecorder(this.$refs["vt-recorder"], e, n)
        }
        ),
        this.attachListeners()
    },
    beforeUnmount() {
        this.close()
    },
    methods: {
        close() {
            this.$emit("close"),
            this.$options.ventHandler.trigger("toswf:close"),
            this.unattachListeners()
        },
        attachListeners() {
            const e = this.$options.ventEvents;
            for (const t in e)
                this.$options.ventHandler.on(t, (n,r)=>{
                    ["fromswf:tabbablesChanged"].includes(t) && this.updateTrap(),
                    this.$emit(e[t], r)
                }
                )
        },
        unattachListeners() {
            const e = this.$options.ventEvents;
            for (const t in e)
                this.$options.ventHandler.off(t)
        },
        updateTrap() {
            this.$refs.trap && (this.$refs.trap.pause(),
            this.$refs.trap.unpause())
        }
    }
}
