const cH = {
    name: "SharedWithList",
    components: {
        ShareListItemWithPermissionsSelect: uH,
        ShareListItem: Xd
    },
    props: {
        updatedItems: {
            type: Array,
            default: ()=>[]
        },
        items: {
            type: Array,
            required: !0
        },
        staticItems: {
            type: Array,
            default: ()=>[]
        },
        permissions: {
            type: Array,
            required: !0
        },
        permissionsOptions: {
            type: Array,
            required: !0
        }
    },
    computed: {
        canEditVerb() {
            return this.permissionsOptions.find(e=>e.value === "edit") ? this.permissionsOptions.find(e=>e.value === "edit").verb : this.translate("permissions.canEdit")
        }
    },
    methods: {
        isUpdated(e) {
            return this.updatedItems.some(t=>t.itemId === (e.userId || e.id) && t.type === e.type)
        },
        permissionLevelForItem(e) {
            const t = this.updatedItems.find(n=>n.itemId === (e.userId || e.id) && n.type === e.type);
            return t ? this.permissionsOptions.find(n=>n.value === t.permission).numeric : parseInt(e.level)
        }
    }
};
