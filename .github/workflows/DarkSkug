const zV = {
    components: {
        SaveCancelFooterBar: qV,
        UnsavedChangesWarningDialog: WV,
        KebabMenu: $g,
        ConfirmDeleteDialog: wC
    },
    props: {
        captionFileOptions: {
            type: Array,
            required: !0,
            validator(e) {
                return e.length > 0 && e.every(t=>$t(["value", "segments"], t))
            }
        },
        selectedCaptionsOnStart: {
            type: Object,
            default: null,
            validator(e) {
                return "value"in e
            }
        }
    },
    emits: ["cancel", "delete:captions", "save:captions"],
    setup() {
        const e = G([])
          , t = G(null)
          , {uuid: n} = on();
        return nr(()=>{
            if (t.value) {
                t.value.focus();
                return
            }
            e.value && e.value[0] && e.value[0].focus()
        }
        ),
        {
            segmentTextAreas: e,
            captionFileSelect: t,
            uuid: n
        }
    },
    data() {
        return {
            editedSegments: [],
            selectedCaptionFile: null,
            modals: {
                unsavedChangesWarning: !1,
                confirmDelete: !1
            },
            saving: !1,
            isResettingCaptionFile: !1,
            fileOptionsHaveChanged: !1,
            continueEditingCallback: null,
            discardChangesCallback: null
        }
    },
    computed: {
        hasUnsavedChanges() {
            return this.segmentsAreDifferent(this.editedSegments, this.selectedCaptionFile.segments)
        },
        hasMultipleCaptions() {
            return this.captionFileOptions.length > 1
        },
        kebabMenuItems() {
            return [{
                items: [{
                    label: this.translate("captions.deleteCaptionFile"),
                    action: a0
                }]
            }]
        }
    },
    watch: {
        captionFileOptions: {
            handler(e) {
                this.saving = !1,
                this.fileOptionsHaveChanged = !0;
                const t = e.find(n=>n.id === this.selectedCaptionFile.id);
                this.selectedCaptionFile = t || e[0];
                for (const n in this.modals)
                    this.modals[n] = !1;
                this.resetSegments()
            }
        },
        selectedCaptionFile(e, t) {
            if (this.isResettingCaptionFile || this.fileOptionsHaveChanged) {
                this.isResettingCaptionFile = !1,
                this.fileOptionsHaveChanged = !1;
                return
            }
            if (t && this.segmentsAreDifferent(this.editedSegments, t.segments)) {
                this.continueEditingCallback = ()=>{
                    this.selectedCaptionFile = t,
                    this.isResettingCaptionFile = !0
                }
                ,
                this.discardChangesCallback = this.resetSegments,
                this.modals.unsavedChangesWarning = !0;
                return
            }
            this.selectedCaptionFile = e,
            this.resetSegments()
        }
    },
    created() {
        let e;
        this.selectedCaptionsOnStart && (e = this.captionFileOptions.find(t=>t.value === this.selectedCaptionsOnStart.value)),
        this.selectedCaptionFile = e || this.captionFileOptions[0],
        this.resetSegments()
    },
    methods: {
        resetSegments() {
            this.editedSegments = this.selectedCaptionFile.segments
        },
        updateSegment(e, t) {
            const n = this.editedSegments.findIndex(s=>s.begin === e.begin)
              , r = ve(V({}, this.editedSegments[n]), {
                content: t
            })
              , a = [...this.editedSegments];
            a.splice(n, 1, r),
            this.editedSegments = a
        },
        handleCancelRequest() {
            if (!this.hasUnsavedChanges) {
                this.$emit("cancel");
                return
            }
            this.continueEditingCallback = null,
            this.discardChangesCallback = ()=>{
                this.$emit("cancel")
            }
            ,
            this.modals.unsavedChangesWarning = !0
        },
        handleDiscardChanges() {
            typeof this.discardChangesCallback == "function" && this.discardChangesCallback(),
            this.closeUnsavedChangesWarning()
        },
        handleContinueEditing() {
            typeof this.continueEditingCallback == "function" && this.continueEditingCallback(),
            this.closeUnsavedChangesWarning()
        },
        closeUnsavedChangesWarning() {
            this.modals.unsavedChangesWarning = !1
        },
        segmentsAreDifferent(e, t) {
            return !Jr.exports.isEqual(e, t)
        },
        handleMenuAction(e) {
            e === a0 && (this.modals.confirmDelete = !0)
        },
        handleDeleteRequest() {
            this.$emit("delete:captions", {
                id: this.selectedCaptionFile.id
            })
        },
        async saveCaptions() {
            this.$emit("save:captions", {
                id: this.selectedCaptionFile.id,
                params: {
                    segments: this.editedSegments
                }
            }),
            this.saving = !0
        }
    }
}
