var gQ = ({element: e, togglePropOrCb: t})=>({
    created() {
        document.addEventListener("click", this.handleOutsideClick)
    },
    beforeUnmount() {
        document.addEventListener("click", this.handleOutsideClick)
    },
    methods: {
        getElementFromRef: wS,
        handleOutsideClick(n) {
            n.stopPropagation();
            const r = this.getElementFromRef(e);
            if (!r || qa(n, r))
                return;
            CS.bind(this)(t)
        }
    }
})
