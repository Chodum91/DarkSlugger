const oJ = {
    components: {
        NavBarDropdown: su
    },
    props: {
        selectedOption: {
            type: Object,
            default: ()=>({})
        },
        myCommentsCount: {
            type: [String, Number],
            default: 0
        },
        allCommentsCount: {
            type: [String, Number],
            default: 0
        },
        withoutCounter: {
            type: Boolean,
            default: !1
        },
        forceAllOptions: {
            type: Boolean,
            default: !1
        },
        closedTextColor: {
            type: String,
            default: "text-white"
        }
    },
    emits: ["update:selectedOption"],
    computed: {
        copyOptions() {
            const e = this.myCommentsCount && !this.withoutCounter ? `(${this.myCommentsCount})` : ""
              , t = this.allCommentsCount && !this.withoutCounter ? `(${this.allCommentsCount})` : ""
              , n = [{
                srLabel: this.translate("labels.menus.copyCommentOptions"),
                items: [bC()]
            }];
            return (this.forceAllOptions || this.myCommentsCount) && n[0].items.push({
                type: "mine",
                label: `${this.translate("labels.myComments")} ${e}`
            }),
            (this.forceAllOptions || this.allCommentsCount) && n[0].items.push({
                type: "all",
                label: `${this.translate("labels.allComments")} ${t}`
            }),
            n
        }
    },
    created() {
        this.selectedOption.type || this.selectOption(this.copyOptions[0].items[0])
    },
    methods: {
        selectOption(e) {
            this.$emit("update:selectedOption", e)
        }
    }
