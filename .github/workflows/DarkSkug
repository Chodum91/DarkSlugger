                return Dc(l) && l.revert && this.revertState ? V({}, this.revertState) : ve(V({}, r), {
                    error: l,
                    errorUpdateCount: r.errorUpdateCount + 1,
                    errorUpdatedAt: Date.now(),
                    fetchFailureCount: r.fetchFailureCount + 1,
                    fetchStatus: "idle",
                    status: "error"
                });
            case "invalidate":
                return ve(V({}, r), {
                    isInvalidated: !0
                });
            case "setState":
                return V(V({}, r), t.state)
            }
        }
        ;
        this.state = n(this.state),
        qn.batch(()=>{
            this.observers.forEach(r=>{
                r.onQueryUpdate(t)
            }
            ),
            this.cache.notify({
                query: this,
                type: "updated",
                action: t
            })
        }
        )
    }
}
function RL(e) {
    const t = typeof e.initialData == "function" ? e.initialData() : e.initialData
      , r = typeof e.initialData != "undefined" ? typeof e.initialDataUpdatedAt == "function" ? e.initialDataUpdatedAt() : e.initialDataUpdatedAt : 0
      , a = typeof t != "undefined";
    return {
        data: t,
        dataUpdateCount: 0,
        dataUpdatedAt: a ? r != null ? r : Date.now() : 0,
        error: null,
        errorUpdateCount: 0,
        errorUpdatedAt: 0,
        fetchFailureCount: 0,
        fetchMeta: null,
        isInvalidated: !1,
        status: a ? "success" : "loading",
        fetchStatus: "idle"
    }
}
class Pw extends ho {
    constructor(t) {
        super();
        this.config = t || {},
        this.queries = [],
        this.queriesMap = {}
    }
    build(t, n, r) {
        var a;
        const s = n.queryKey
          , l = (a = n.queryHash) != null ? a : ng(s, n);
        let u = this.get(l);
        return u || (u = new BL({
            cache: this,
            logger: t.getLogger(),
            queryKey: s,
            queryHash: l,
            options: t.defaultQueryOptions(n),
            state: r,
            defaultOptions: t.getQueryDefaults(s),
            meta: n.meta
        }),
        this.add(u)),
        u
    }
    add(t) {
        this.queriesMap[t.queryHash] || (this.queriesMap[t.queryHash] = t,
        this.queries.push(t),
        this.notify({
            type: "added",
            query: t
        }))
    }
    remove(t) {
        const n = this.queriesMap[t.queryHash];
        n && (t.destroy(),
        this.queries = this.queries.filter(r=>r !== t),
        n === t && delete this.queriesMap[t.queryHash],
        this.notify({
            type: "removed",
            query: t
        }))
    }
    clear() {
        qn.batch(()=>{
            this.queries.forEach(t=>{
                this.remove(t)
            }
            )
        }
        )
    }
    get(t) {
        return this.queriesMap[t]
    }
    getAll() {
        return this.queries
    }
    find(t, n) {
        const [r] = ys(t, n);
        return typeof r.exact == "undefined" && (r.exact = !0),
        this.queries.find(a=>m0(r, a))
    }
    findAll(t, n) {
        const [r] = ys(t, n);
        return Object.keys(r).length > 0 ? this.queries.filter(a=>m0(r, a)) : this.queries
    }
    notify(t) {
        qn.batch(()=>{
            this.listeners.forEach(n=>{
                n(t)
            }
            )
        }
        )
    }
    onFocus() {
        qn.batch(()=>{
            this.queries.forEach(t=>{
                t.onFocus()
            }
            )
        }
        )
    }
    onOnline() {
        qn.batch(()=>{
            this.queries.forEach(t=>{
                t.onOnline()
            }
            )
        }
        )
    }
}
class ML extends Ow {
    constructor(t) {
        super();
        this.options = V(V({}, t.defaultOptions), t.options),
        this.mutationId = t.mutationId,
        this.mutationCache = t.mutationCache,
        this.logger = t.logger || rg,
        this.observers = [],
        this.state = t.state || Aw(),
        this.meta = t.meta,
        this.updateCacheTime(this.options.cacheTime),
        this.scheduleGc()
    }
    setState(t) {
        this.dispatch({
            type: "setState",
            state: t
        })
    }
    addObserver(t) {
        this.observers.indexOf(t) === -1 && (this.observers.push(t),
        this.clearGcTimeout(),
        this.mutationCache.notify({
            type: "observerAdded",
            mutation: this,
            observer: t
        }))
    }
    removeObserver(t) {
        this.observers = this.observers.filter(n=>n !== t),
        this.scheduleGc(),
        this.mutationCache.notify({
            type: "observerRemoved",
            mutation: this,
            observer: t
        })
    }
    optionalRemove() {
        this.observers.length || (this.state.status === "loading" ? this.scheduleGc() : this.mutationCache.remove(this))
    }
    continue() {
        return this.retryer ? (this.retryer.continue(),
        this.retryer.promise) : this.execute()
    }
    async execute() {
        const t = ()=>{
            var q;
            return this.retryer = kw({
                fn: ()=>this.options.mutationFn ? this.options.mutationFn(this.state.variables) : Promise.reject("No mutationFn found"),
                onFail: ()=>{
                    this.dispatch({
                        type: "failed"
                    })
                }
                ,
                onPause: ()=>{
                    this.dispatch({
                        type: "pause"
                    })
                }
                ,
                onContinue: ()=>{
                    this.dispatch({
                        type: "continue"
                    })
                }
                ,
                retry: (q = this.options.retry) != null ? q : 0,
                retryDelay: this.options.retryDelay,
                networkMode: this.options.networkMode
            }),
            this.retryer.promise
        }
          , n = this.state.status === "loading";
        try {
            var r, a, s, l, u, c;
            if (!n) {
                var d, p, h, v;
                this.dispatch({
                    type: "loading",
                    variables: this.options.variables
                }),
                (d = (p = this.mutationCache.config).onMutate) == null || d.call(p, this.state.variables, this);
                const K = await ((h = (v = this.options).onMutate) == null ? void 0 : h.call(v, this.state.variables));
                K !== this.state.context && this.dispatch({
                    type: "loading",
                    context: K,
                    variables: this.state.variables
                })
            }
            const q = await t();
            return (r = (a = this.mutationCache.config).onSuccess) == null || r.call(a, q, this.state.variables, this.state.context, this),
            await ((s = (l = this.options).onSuccess) == null ? void 0 : s.call(l, q, this.state.variables, this.state.context)),
            await ((u = (c = this.options).onSettled) == null ? void 0 : u.call(c, q, null, this.state.variables, this.state.context)),
            this.dispatch({
                type: "success",
                data: q
            }),
            q
        } catch (q) {
            try {
                var b, w, $, P, A, X;
                throw (b = (w = this.mutationCache.config).onError) == null || b.call(w, q, this.state.variables, this.state.context, this),
                await (($ = (P = this.options).onError) == null ? void 0 : $.call(P, q, this.state.variables, this.state.context)),
                await ((A = (X = this.options).onSettled) == null ? void 0 : A.call(X, void 0, q, this.state.variables, this.state.context)),
                q
            } finally {
                this.dispatch({
                    type: "error",
                    error: q
                })
            }
        }
    }
    dispatch(t) {
        const n = r=>{
            switch (t.type) {
            case "failed":
                return ve(V({}, r), {
                    failureCount: r.failureCount + 1
                });
            case "pause":
                return ve(V({}, r), {
                    isPaused: !0
                });
            case "continue":
                return ve(V({}, r), {
                    isPaused: !1
                });
            case "loading":
                return ve(V({}, r), {
                    context: t.context,
                    data: void 0,
                    error: null,
                    isPaused: !Nd(this.options.networkMode),
                    status: "loading",
                    variables: t.variables
                });
            case "success":
                return ve(V({}, r), {
                    data: t.data,
                    error: null,
                    status: "success",
                    isPaused: !1
                });
            case "error":
                return ve(V({}, r), {
                    data: void 0,
                    error: t.error,
                    failureCount: r.failureCount + 1,
                    isPaused: !1,
                    status: "error"
                });
            case "setState":
                return V(V({}, r), t.state)
            }
        }
        ;
        this.state = n(this.state),
        qn.batch(()=>{
            this.observers.forEach(r=>{
                r.onMutationUpdate(t)
            }
            ),
            this.mutationCache.notify({
                mutation: this,
                type: "updated",
                action: t
            })
        }
        )
    }
}
function Aw() {
    return {
        context: void 0,
        data: void 0,
        error: null,
        failureCount: 0,
        isPaused: !1,
        status: "idle",
        variables: void 0
    }
}
class Dw extends ho {
    constructor(t) {
        super();
        this.config = t || {},
