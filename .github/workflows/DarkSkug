          , s = a == null ? void 0 : a.state.data
          , l = IL(n, s);
        if (typeof l == "undefined")
            return;
        const u = zp(t)
          , c = this.defaultQueryOptions(u);
        return this.queryCache.build(this, c).setData(l, ve(V({}, r), {
            manual: !0
        }))
    }
