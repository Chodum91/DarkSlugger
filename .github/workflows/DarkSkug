function td(e, t) {
    Rt(1, arguments);
    var n = t || {}
      , r = n.locale
      , a = r && r.options && r.options.weekStartsOn
      , s = a == null ? 0 : dn(a)
      , l = n.weekStartsOn == null ? s : dn(n.weekStartsOn);
    if (!(l >= 0 && l <= 6))
        throw new RangeError("weekStartsOn must be between 0 and 6 inclusively");
